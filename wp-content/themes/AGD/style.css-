/************************************************
*	6.	Homepage 					     	    *
************************************************/
/*General*/
.homeSection{
    display:table;
	table-layout:fixed;
	width:100%;
	position:relative;
	overflow:visible;
	direction:ltr;
}
.homeSection.RTL{
	direction:rtl;
}
.homesection:after{
	content:" ";
	display:block;
	clear:both;
}
.homeSectionCell{
	display:table-cell;
	vertical-align:top;
	direction:ltr;
	text-align:left;
	overflow:visible;
	background-size:cover;
	background-repeat:no-repeat;
	background-position:center;
	position:relative;
}
.MediaCell{
	width:65%;
}
.MediaCell img{
	display:block;
	visibility:hidden;
	position:relative;
}
.ContentCell{
	width:35%;
	position:relative;
}

.homeSectionContentBox{
	overflow:auto;
	padding-top:12%;
	padding-bottom:12%;
	padding-left:15%;
	padding-right:35px;
	text-align:right;
}
.homeSection.RTL .homeSectionContentBox{
	padding-left:35px;
	padding-right:15%;
	text-align:left;
}


.sectionTitleWrap{
	display:table;
	table-layout:fixed;
	width:100%;
	margin-bottom:8%;
}
.sectionTitleCol{
	display:table-cell;
	vertical-align:middle;
}

.section_content_full{
	padding:8% 0px;
	position:relative;
	z-index:2
}
.section_title{
	font-size:4em;
	line-height:1.2;
	margin:0px 0px 5% 0px;
	padding:0px;
}



.placeholder_img{
	display:none !important;
}
/*Home Banner*/
.BannerWrap{
	position: relative;
	background-color: #31373e;
	padding-top: 7%;
}
.BannerTable{
	display: table;
	width:100%;
}
.BannerImg, .PriceCalc{
	display: table-cell;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}
.BannerImg{
	width: 60%;
	vertical-align: bottom;
	color: #fff;
	padding: 5% 4%;
	background-size: cover;
}
.BannerImg .arrowButton,
.BannerImg .longArrow,
.BannerImg .longArrow:before,
.BannerImg .longArrow:after{
	color: #fff !important;
}

.PriceCalc{
	width: 40%;
	vertical-align:middle;
}

#home{
	padding:8% 0px;
}
.bannerContentWrap{
	padding-top:0px;
}
.bannerContentBox{
	padding:15%;
	text-align:left;
	position:relative;
	z-index:1;
	background-color:#fff;
	color:#31373e;
}
.bannerContentBox h1, .bannerContentBox h2, .bannerContentBox h3, .bannerContentBox h4, .bannerContentBox h5, .bannerContentBox h6{
	color:#d7bf89;
}
.img_wrap{
	position:relative;
}
.banner_title_1{
	position:absolute;
	right:20px;
	bottom:110px;
	color:#fff;
	margin:0px;
	padding:0px;
	line-height:1;
	font-size:46px;
}
.banner_title_2{
	position:absolute;
	right:-20px;
	bottom:-35px;
	font-size:165px;
	line-height:1;
	color:#fff;
	margin:0px;
	padding:0px;
}

.LiveGoldPriceWarp{
	position:relative;
	top:0;
	left:0;
	width:100%;
	max-width:650px;
	min-height:320px;
}
.LiveGoldPrice{
	background-color:#d7bf89;
	padding: 23% 14%;
	box-sizing:border-box;
	-webkit-box-sizing:border-box;
}
.spotPrice_title{
	color:#31373e;
	font-size:18px;
	font-family:"ProximaNova-Semibold";
}
.spotPrice{
	font-size:36px;
	color:#fff;
	font-family:"ProximaNova-Semibold";
	padding-bottom:15px;
}
.GoldPriceList{
	display:block;
	margin:0px;
	padding:10px 0 0 0;
}
.GoldPriceList li{
	display:block;
	margin:0px;
	padding:7px 0px;
	font-family:"ProximaNova-Semibold";
	font-size:18px;
	color:#fff;
}
.BannerNotice{
	color:#31373e;
	font-size:18px;
	font-family:"ProximaNova-Semibold";
}

