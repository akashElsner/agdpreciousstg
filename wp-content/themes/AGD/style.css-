/*body, html{
	cursor:url(images/cursor.png), default;
}*/
body {
	background-color: #ffffff;
	color: #31373e;
	font-size: 16px;
	font-family: 'ProximaNova-Regular';
	font-weight:400;
	line-height:1.4;
	letter-spacing:normal;
	margin: 0px;
	padding: 0px;
	-webkit-overflow-scrolling: touch;
	overflow:scroll
	}

/************************************************
*	0.	Global									*
************************************************/
p {
	padding: 15px 0px;
	margin: 0px;
	}
p:empty {
	display: none;
	}
em {
	font-style: italic;
	}
strong, b {
	font-family: 'ProximaNova-Semibold';
	font-weight:normal;
	}
hr {
	border: 0;
	width: 100%;
	height: 1px;
	color: #DDDDDD;
	background-color: #DDDDDD;
	margin: 6px 0px 8px 0px;
	padding: 0px;
	}
ol, ul {
	margin: 6px 0px;
	padding: 0px 0px 0px 12px;
	}
ol li {
	list-style: inside;
	list-style-type: decimal;
	margin: 6px 0px;
	padding: 0px 0px 0px 12px;
	}
ul{
	margin:0px;
	padding:10px 0px 10px 20px;
}
ul li {
	line-height:normal;
	list-style-type:circle;
	padding:6px 0;
	}
ul ol li, ul ol ul ol li {
    list-style-type: decimal;
	}
ol ul li, ol ul ol ul li {
    list-style-type: square;
	}
blockquote {
	font-family: 'ProximaNova-Regular';
	font-weight: 300;
	font-size: 1.6em;
	line-height: 1.6;
	margin: 20px 0px;
	padding: 20px;
	}
blockquote p {
	margin: 0px;
	padding: 0px;
	}
blockquote, q {
	quotes: none;
	}
blockquote:before, blockquote:after,
q:before, q:after {
	content: '';
	content: none;
	}
code, pre {
	background: #F9F9F9;
	font-family: Courier, Verdana, Tahoma, Verdana;
	font-size: 13px;
	line-height: 1.4;
	margin: 12px 0px;
	padding: 6px 8px;
	border: 1px solid #DDDDDD;
	border-radius: 3px;
	}
code {
	display: inline;
	white-space: normal;
	}
pre {
	display: block;
	}
pre code {
	padding: 0px;
	border: none;
	border-radius: 0px;
	}
img {
	-ms-interpolation-mode: bicubic;
	height: auto;
	}
img, object, embed {
	max-width: 100%;
	margin: 0px auto 0px;
	}
table {
	border-collapse: collapse;
	border-spacing: 0;
	}
.clear {
	clear: both;
	}
.hidden {
	display: none;
	}
.text-left {
	text-align: left;
	}
.text-right {
	text-align: right;
	}
.text-center {
	text-align: center;
	}
.text-hide,
.container .headline.text-hide {
	font-size: 0;
	line-height: 0;
	margin-left: -999em;
	margin-top: 0px;
	margin-bottom: 0px;
	padding: 0px;
	}
.align-center {
	float: none;
	display: block;
	text-align: center;
	margin-left: auto;
	margin-right: auto;
	}
.align-left {
	float: left;
	}
.align-right {
	float: right;
	}
.vertical-center {
	top: 50%;
	-webkit-transform: translateY(-50%);
	-ms-transform: translateY(-50%);
	transform: translateY(-50%);
	}

/************************************************
*	1.	Hyperlinks								*
************************************************/

a img {
	border: none;
	}
a, a:link {
	color: #1d1f21;
	text-decoration:none;
	transition:all ease 0.3s;
	-webkit-transition:all ease 0.3s;
	}
a:focus, a:hover, a:active {
	color: #1d1f21;
	text-decoration: underline;
	}

/************************************************
*	2.	Headings  								*
************************************************/

h1, h2, h3, h4, h5, h6 {
	font-family: 'Playfair Display', serif;
	font-style:italic;
	font-weight:400;
	line-height: normal;
	letter-spacing:normal;
	}
/*Alternate heading style*/
h1 strong, h2 strong, h3 strong, h4 strong, h5 strong, h6 strong,
h1 b, h2 b, h3 b, h4 b, h5 b, h6 b{
	font-weight:700;
	font-style:normal;
}
h1 {
	font-size:75px;
	margin: 4px 0px;
	padding: 4px 0px;
	}
h2 {
	font-size: 60px;
	margin: 4px 0px ;
	padding: 4px 0px;
	}
h3 {
	font-size: 45px;
	margin: 4px 0px;
	padding: 4px 0px;
	}
h4 {
	font-size: 35px;
	margin: 10px 0px;
	padding: 4px 0px;
	}
h5 {
	font-size: 25px;
	margin: 4px 0px;
	padding: 4px 0px;
	}
h6 {
	font-size: 20px;
	margin: 4px 0px;
	padding: 4px 0px;
	}
h1 a, h2 a, h3 a, h4 a, h5 a, h6 a {
	color: #aecb5d;
	text-decoration: none;
	border: none;
	}
h1 a:hover, h2 a:hover, h3 a:hover,
h4 a:hover, h5 a:hover, h6 a:hover {
	color: #aecb5d;
	text-decoration: none;
	border: none;
	}

/************************************************
*	3.	Header  								*
************************************************/

#header {
	left:0;
	right:0;
	width:100%;
	line-height: 0;
	margin: 0px;
	padding:0px;
	overflow:visible;
	background-color:#31373e;
	color:#fff;
	position:relative;
	border-bottom:1px solid rgba(255, 255, 255, 0.2);
	z-index:99;
	}
#header .header_table{
	display:table;
	table-layout:fixed;
	width:100%;
}
#header .header_table_cell{
	display:table-cell;
	vertical-align:middle;
	padding:24px;
	box-sizing:border-box;
	text-align:center;
	-webkit-box-sizing:border-box;
}
#header .header_left{
	width:25%;
	border-right:1px solid rgba(255, 255, 255, 0.2);
}
#header .header_mid{
	border-right:1px solid rgba(255, 255, 255, 0.2);
}
#header .header_right{
	width:15%;
}

#header a.logo{
	display:block;
	width:87px;
	margin:0 auto;
}
.header_call{
	display:block;
	padding:20px 0px 0px;
}
.header_call a{
	display:inline-block;
	text-decoration:none;
	font-size:16px;
	color:#fff;
	line-height:1;
}
.header_call a:hover{
	color:#d7bf89
}
.header_call a .fa{
	font-size:24px;
}


/************************************************
*	4.	Navigation 								*
************************************************/
/*Menu*/
.menu-toggle {
	display:inline-block;
	cursor:pointer;
	margin: 0px 5px;
	padding: 5px;
	z-index: 9;
	color:#111111;
	text-align:center;
	box-shadow:none;
	-webkit-box-shadow:none;
	box-sizing:border-box;
	-webkit-box-sizing:border-box;
	transition:all ease-in-out 0.2s;
	-webkit-transition:all ease-in-out 0.2s;
}
#menu-toggle > span {
	background-color: #d7bf89;
	content: " ";
	display: block;
	height: 2px;
	margin-bottom: 9px;
	width: 39px;
}
#navigation{
	display:none;
}

/**MMENU**/
.mm-menu{
	font-family: 'Playfair Display', serif;
	font-style:italic;
	font-size:19px;
	color:#d7bf89;
}
.mm-navbar{
	background-color:rgba(0,0,0,0.01) !important;
}
.mm-listview{
	font-size:1em;
}
.mm-title{
	text-decoration:none !important;
}
.mm-listview > li > a, .mm-listview > li > span{
	padding:15px 15px 15px 20px;
	text-decoration:none;
	color:#d7bf89;
}
.mm-listview > li:hover a, .mm-listview > li:hover span{
	color:#fff;
	text-decoration:none;
}
.mm-listview > li > a img, .mm-listview > li > span img{
	height:32px;
	width:auto;
	position:absolute;
	left:20px;
}
.mm-listview > li > a > span, .mm-listview > li > span > span{
	padding-left:50px;
}
.mm-listview > li.mm-selected > span, .mm-listview > li.mm-selected > a{
	background-color:transparent !important;
	color:#fff;
}

.mm-menu.mm-theme-black .mm-listview > li.mm-selected > a:not(.mm-next), .mm-menu.mm-theme-black .mm-listview > li.mm-selected > span{
	background-color:transparent;
	color:#fff;
}
.mm-listview > li, .mm-listview > li .mm-next, .mm-listview > li .mm-next:before, .mm-listview > li:after, .mm-navbar, .mm-menu .mm-listview > li .mm-arrow:after, .mm-menu .mm-listview > li .mm-next:after, .mm-menu .mm-btn:after, .mm-menu .mm-btn:before{
	border-color:transparent;
}
.mm-menu .mm-navbar a, .mm-menu .mm-navbar > *{
	color:rgba(0,0,0,0.1);
}
.mm-menu.mm-theme-dark .mm-btn:after, .mm-menu.mm-theme-dark .mm-btn:before,
.mm-menu.mm-theme-dark .mm-listview > li .mm-arrow:after, .mm-menu.mm-theme-dark .mm-listview > li .mm-next:after{
	border-color:#fff;
}
.mm-menu.mm-theme-dark .mm-navbar a, .mm-menu.mm-theme-dark .mm-navbar > *, .mm-menu.mm-theme-dark em.mm-counter{
	color:#d7bf89;
}

/*Header Menu*/
#header_menu ul.menu{
	display:block;
	margin:0px;
	padding:0px;
}
#header_menu ul.menu li{
	display:inline-block;
	margin:0px;
	padding:0px 10px;
}
#header_menu ul.menu li:first-child{
	padding-left:0px;
}
#header_menu ul.menu li:last-child{
	padding-right:0px;
}
#header_menu ul.menu li a{
	display:block;
	text-decoration:none;
	text-transform:uppercase;
	color:#fff;
}
#header_menu ul.menu li a:hover{
	color:#d7bf89;
}

